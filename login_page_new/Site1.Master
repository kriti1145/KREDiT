                        <li class="nav-item">
                            <asp:LinkButton class="nav-link text-light " ID="LinkButton3" runat="server" href="signin_page.aspx"><strong>FIND TRAINER</strong></asp:LinkButton>
                        </li>
                         <li class="nav-item">
                            <asp:LinkButton class="nav-link text-light " ID="LinkButton6" runat="server" href="signin_page.aspx"><strong>REVIEW</strong></asp:LinkButton>
                        </li>
                    </ul>

                    <div class=" nav navbar-nav flex-row justify-content-center flex-nowrap m-1">

                        <asp:LinkButton ID="lnkSignin" PostBackUrl="~/signin_page.aspx" runat="server" CssClass="btn btn-outline-light me-1" Style="width: 100px; border-radius: 5px; font-family: monospace; color: #E2D59B;" >SIGN-IN</asp:LinkButton>
                        <asp:Label ID="lblUserId" runat="server"></asp:Label>
                        <asp:LinkButton ID="lnkSignup" runat="server" PostBackUrl="~/signup_page.aspx" CssClass="btn btn-outline-light me-1 ms-1" Style="width: 100px; border-radius: 5px; font-family: monospace; color: #E2D59B;" Visible="false">SIGN-UP</asp:LinkButton>
                        <asp:LinkButton ID="lnkLogout" runat="server" PostBackUrl="~/Home.aspx" CssClass="btn btn-outline-light ms-1" Style="width: 100px; border-radius: 5px; font-family: monospace; color: #E2D59B;" Visible="false" >LOGOUT</asp:LinkButton>

