    <asp:ContentPlaceHolder ID="head" runat="server">
         <style>
            * {
                font-family: Cambria, Cochin, Georgia, Times, Times New Roman, serif;
            }

            label {
                font-size: calc(15px + 0.390625vw);
            }

            h1 {
                color: #002800;
            }

            i {
                font-size: 50px;
                margin-bottom: 15px;
                color: #002800;
            }

            h3 {
